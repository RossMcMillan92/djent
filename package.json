	},
	"jest": {
		"modulePaths": [
			"<rootDir>/src/generic/",
			"<rootDir>/src/generic/scripts/",
			"<rootDir>/src/generic/scripts/app/",
			"<rootDir>/src/generic/styles/styles/"
