    />
    <link
      rel="apple-touch-icon"
      sizes="76x76"
      href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/76.png"
    />
    <link
      rel="apple-touch-icon"
      sizes="114x114"
      href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/114.png"
    />
    <link
      rel="apple-touch-icon"
      sizes="120x120"
      href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/120.png"
    />
    <link
      rel="apple-touch-icon"
      sizes="144x144"
      href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/144.png"
    />
    <link
      rel="apple-touch-icon"
      sizes="152x152"
      href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/152.png"
    />
    <link
      rel="apple-touch-icon"
      sizes="180x180"
      href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/180.png"
    />
    <link
      rel="icon"
      type="image/png"
      sizes="192x192"
      href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/192.png"
    />
    <link
      rel="icon"
      type="image/png"
      sizes="96x96"
      href="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/96.png"
    />
    <meta name="msapplication-TileColor" content="#1e1e1e" />
    <meta
      name="msapplication-TileImage"
      content="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/icons/144.png"
    />

    <meta
      http-equiv="Content-Security-Policy"
      content="default-src * blob:;
	                  style-src * 'self' 'unsafe-inline' 'unsafe-eval';
	                  script-src * 'self' 'unsafe-inline' 'unsafe-eval' data:;
	                  worker-src * blob: 'self' 'unsafe-inline' 'unsafe-eval';"
    />
    <noscript>
      <style>
        .js {
          display: none !important;
        }
      </style>
    </noscript>
    <script
      data-ad-client="ca-pub-9971092397934564"
      async
      src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"
    ></script>
  </head>

  <body class="preload">
    <div
      class="header<% if (htmlWebpackPlugin.options.isPhoneGap) { %> header--nav-spaced <% } %>"
      ref="header"
    >
      <div class="group-capped-x group-centered">
        <div class="group-spacing-x">
          <div class="u-flex-row u-flex-justify">
            <img
              class="header__logo"
              src="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/logo.png"
              alt="DJEN metal generator logo"
            />

            <div class="u-flex-row">
              <a
                class=""
                href="https://www.facebook.com/djenerationstation/"
                target="_blank"
                rel="noopener"
              >
                <img
                  class="header__icon social-icon"
                  src="<%= htmlWebpackPlugin.options.absolutePath %>assets/images/F_icon.svg"
                  width="39"
                  height="39"
                  alt="facebook icon"
                />
              </a>
            </div>
          </div>
          <div class="top-ad-container">
            <!-- Ezoic - top_of_page - top_of_page -->
            <div id="ezoic-pub-ad-placeholder-101"></div>
            <!-- End Ezoic - top_of_page - top_of_page -->
          </div>
        </div>
      </div>
    </div>
    <div id="root"></div>
    <noscript>
      <p class="u-mt2 u-tac">JavaScript is required for djen.co to run</p>
    </noscript>
